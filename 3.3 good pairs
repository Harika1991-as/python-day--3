def good(n):
    count=0
    a=len(n)
    for i in range(a):
        for j in range(i+1,a):
            if(n[i]==n[j]):
                count=count+1
    return count
n=[]
b=int(input("enter the number of elements"))
for k in range(b):
    c=int(input("enter the element"))
    n.append(c)
print(good(n))
            
